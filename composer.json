{
    "name": "saad/passport-auth-proxy",
    "description": "This library used as a proxy for laravel passport authentication to generate jwt access and refresh tokens",
    "keywords": [
        "passport",
        "oauth",
        "oauth2",
        "api",
        "authentication",
        "tymon",
        "jwt",
        "proxy",
        "scope",
        "laravel"
    ],
    "require": {
        "optimus/api-consumer": "^0.2.3",
        "laravel/passport": "^5.0",
        "saad/json-response-builder": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0",
        "phpunit/phpunit": "~7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Saad",
            "email": "a7mad.sa3d.2014@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Saad\\Passport\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saad\\Passport\\AuthProxyServiceProvider"
            ],

            "aliases": {
            }
        }
    }
}
